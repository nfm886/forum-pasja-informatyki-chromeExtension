{
    "manifest_version": 2,
    "name": "Powiadomienia z Pasją",
    "description": "Szybki dostęp do powiadomień i obserwowawnych tagów z forum.pasja-informatyki.pl",
    "version": "5.0.2",
    "icons": {
      "16": "./assets/images/icon.png",
       "48": "./assets/images/icon-48x48.png",
      "128": "./assets/images/icon-128x128.png"
    },
    "options_page": "./views/settings.html",
    "background": {
      "scripts": ["./src/global/jquery.js","./src/global/functions.js","./src/background.js"]
    },
    "browser_action": {
      "default_icon": "./assets/images/icon.png",
      "default_popup": "./views/popup.html"
    },
    "permissions": ["*://*.pasja-informatyki.pl/", "storage"]
  }
